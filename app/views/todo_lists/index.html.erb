<table class="table">
<thead class="thead-dark">
<%= paginate @list %>
<tr>
  <th>Title</th>
  <th>Description</th>
  <th>Date</th>
  <th>Priority</th>
  <th colspan="3"></th>
</tr>
</thead>
  <tbody>
  <%= link_to 'New Todo List', new_todo_list_path %>
  <% @list.each do |list| %>
    <tr>
      <td> <%= list.title %> </td>
      <td> <%= list.description %> </td>
      <td> <%= list.due_date %></td>
      <td> <%= list.priority %></td>
      <td> <%= link_to 'Show',list %></td>
      <td> <%= link_to 'Delete',list, method: :delete %></td>
    </tr>
  <% end %>
  </tbody>

</table>
